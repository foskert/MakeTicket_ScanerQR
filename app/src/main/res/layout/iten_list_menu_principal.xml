<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="10dp"
    android:paddingTop="10dp"
    android:paddingBottom="10dp"
    android:paddingRight="10dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/iten_order_entered"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/rect_sald_white"
        android:layout_marginTop="1dp"
        android:padding="10dp"
        tools:layout_editor_absoluteX="1dp">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/baseline_insert_drive_file_24" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="Lista de ordenes ingresadas"
            android:textSize="20dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/iten_order_entry"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/rect_sald_white"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@+id/iten_order_entered"
        android:padding="10dp"
        tools:layout_editor_absoluteX="1dp">

        <ImageView
            android:id="@+id/iten_ico_order_entry"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/baseline_file_open_24_gray" />

        <TextView
            android:id="@+id/iten_text_silla"
            style="@style/code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="Lista de ordenes por ingresar"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iten_ico_order_entry"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/iten_order_dni"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/rect_sald_white"
        android:layout_marginTop="3dp"
        android:padding="10dp"
        app:layout_constraintTop_toBottomOf="@+id/iten_order_entry"
        tools:layout_editor_absoluteX="1dp">

        <ImageView
            android:id="@+id/iten_ico_order_dni"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/baseline_supervised_user_circle_24" />

        <TextView
            android:id="@+id/iten_text_order_dni"
            style="@style/code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_toEndOf="@+id/iten_ico_order_dni"
            android:text="Order de compra por usuario"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iten_ico_order_dni"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/iten_silla"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/rect_sald_white"
        android:layout_marginTop="3dp"
        android:padding="10dp"
        app:layout_constraintTop_toBottomOf="@+id/iten_order_dni"
        tools:layout_editor_absoluteX="1dp">

        <ImageView
            android:id="@+id/iten_ico_menu"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_baseline_view_silla" />

        <TextView
            android:id="@+id/iten_text_order_entry"
            style="@style/code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_toEndOf="@+id/iten_ico_menu"
            android:text="Detalle de silla"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iten_ico_menu"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/iten_mesa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/rect_sald_white"
        android:layout_marginTop="3dp"
        android:padding="10dp"
        app:layout_constraintTop_toBottomOf="@+id/iten_silla"
        tools:layout_editor_absoluteX="1dp">

        <ImageView
            android:id="@+id/iten_ico_menu_mesa"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_baseline_view_mesa" />

        <TextView
            android:id="@+id/iten_text_mesa"
            style="@style/code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_toEndOf="@+id/iten_ico_menu_mesa"
            android:text="Detalle de mesa"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iten_ico_menu_mesa"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/iten_report_ventas"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/rect_sald_white"
        android:layout_marginTop="3dp"
        android:padding="10dp"
        app:layout_constraintTop_toBottomOf="@+id/iten_mesa"
        tools:layout_editor_absoluteX="1dp">

        <ImageView
            android:id="@+id/iten_ico_menu_reporte"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_baseline_analytics_24" />

        <TextView
            android:id="@+id/iten_text_reporte"
            style="@style/code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_toEndOf="@+id/iten_ico_menu_reporte"
            android:text="Reportes de ventas"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iten_ico_menu_reporte"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/iten_report_icomes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/rect_sald_white"
        android:layout_marginTop="3dp"
        android:padding="10dp"
        app:layout_constraintTop_toBottomOf="@+id/iten_report_ventas"
        tools:layout_editor_absoluteX="1dp">

        <ImageView
            android:id="@+id/iten_ico_menu_reporte_icomes"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_baseline_analytics_24" />

        <TextView
            android:id="@+id/iten_text_reporte_icomes"
            style="@style/code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_toEndOf="@+id/iten_ico_menu_reporte_icomes"
            android:text="Reportes de icomes"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iten_ico_menu_reporte_icomes"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/iten_report_detail_order"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/rect_sald_white"
        android:layout_marginTop="3dp"
        android:padding="10dp"
        app:layout_constraintTop_toBottomOf="@+id/iten_report_icomes"
        tools:layout_editor_absoluteX="1dp">

        <ImageView
            android:id="@+id/iten_ico_menu_reporte_detail_order"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_baseline_analytics_24" />

        <TextView
            android:id="@+id/iten_text_reporte_detail_order"
            style="@style/code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="Reportes de detalle de ordenes"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iten_ico_menu_reporte_detail_order"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/iten_menu_list_event"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/rect_sald_white"
        android:layout_marginTop="3dp"
        android:padding="10dp"
        app:layout_constraintTop_toBottomOf="@+id/iten_report_detail_order"
        tools:layout_editor_absoluteX="1dp">

        <ImageView
            android:id="@+id/iten_ico_menu_list_event"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/baseline_grading_24" />

        <TextView
            style="@style/code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="Lista de eventos "
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iten_ico_menu_list_event"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/iten_espacios_silla"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/rect_sald_white"
        android:layout_marginTop="3dp"
        android:padding="10dp"
        app:layout_constraintTop_toBottomOf="@+id/iten_menu_list_event"
        tools:layout_editor_absoluteX="1dp">

        <ImageView
            android:id="@+id/iten_ico_menu_espacios_silla"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/baseline_map_24" />

        <TextView
            android:id="@+id/iten_text_espacios_silla"
            style="@style/code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="Distribución de espacio de silla "
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iten_ico_menu_espacios_silla"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/iten_espacios_mesa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/rect_sald_white"
        android:layout_marginTop="3dp"
        android:padding="10dp"
        app:layout_constraintTop_toBottomOf="@+id/iten_espacios_silla"
        tools:layout_editor_absoluteX="1dp">

        <ImageView
            android:id="@+id/iten_ico_menu_espacios_mesa"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/baseline_map_24" />

        <TextView
            android:id="@+id/iten_text_espacios_mesa"
            style="@style/code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="Distribución de espacio de mesas "
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iten_ico_menu_espacios_mesa"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>